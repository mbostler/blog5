<p id="notice"><%= notice %></p>

<h1>Homepage Pics</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Alt text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @homepage_pics.each do |homepage_pic| %>
      <tr>
        <td><%= homepage_pic.content %></td>
        <td><%= homepage_pic.alt_text %></td>
        <td><%= link_to 'Show', homepage_pic %></td>
        <td><%= link_to 'Edit', edit_homepage_pic_path(homepage_pic) %></td>
        <td><%= link_to 'Destroy', homepage_pic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Homepage Pic', new_homepage_pic_path %>
